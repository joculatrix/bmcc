/* using this example to test function prototyping works as expected */

fib: function integer (n: integer);

main: function integer () = {
    print fib(10);
    return 0;
}

fibonacci: function integer (n: integer) = {
    if (n == 0) {
        return 0;
    } else if (n == 1) {
        return 1;
    } else {
        return fib(n - 1) + fib(n - 2);
    }
}